<?xml version='1.0' encoding='utf-8'?>
<widget id="ablprojects.com" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Beacons Solutions</name>
    <description>
      Aplicaci√≥n para lectura de datos emitidas por los Beacons
    </description>
    <author email="ablprojectssas@gmail.com" href="http://ablprojectss.ml">
        Equipo de ABLprojects
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-eddystone" spec="^1.3.0" />
    <engine name="android" spec="^7.0.0" />
</widget>
